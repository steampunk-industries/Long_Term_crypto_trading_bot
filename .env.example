# Exchange API Keys (Replace with your own keys)
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
COINBASE_API_KEY=your_coinbase_api_key
COINBASE_API_SECRET=your_coinbase_api_secret
KUCOIN_API_KEY=your_kucoin_api_key
KUCOIN_API_SECRET=your_kucoin_api_secret

# On-Chain Data API Keys
GLASSNODE_API_KEY=your_glassnode_api_key
CRYPTOQUANT_API_KEY=your_cryptoquant_api_key

# Sentiment Analysis API Keys
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
NEWS_API_KEY=your_news_api_key

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=crypto_bot
DB_USER=postgres
DB_PASSWORD=password

# Trading Configuration
TRADING_SYMBOL=BTC/USDT
INITIAL_CAPITAL=10000
PAPER_TRADING=true

# Risk Parameters
LOW_RISK_STOP_LOSS=0.02
MEDIUM_RISK_STOP_LOSS=0.03
HIGH_RISK_STOP_LOSS=0.05

# Leverage
MEDIUM_RISK_LEVERAGE=2
HIGH_RISK_LEVERAGE=5

# Exchange Fees
TAKER_FEE=0.0004
MAKER_FEE=0.0002

# Logging
LOG_LEVEL=INFO
LOG_TO_CLOUDWATCH=false

# Portfolio Management
MAX_PORTFOLIO_DRAWDOWN=0.15
MAX_CORRELATION=0.7
MAX_ALLOCATION_PER_ASSET=0.25
RISK_FREE_RATE=0.02

# Profit Withdrawal Settings
PROFIT_THRESHOLD=50000
PROFIT_WITHDRAWAL_PERCENTAGE=0.5

# Market Regime Detection
VOLATILITY_WINDOW=20
TREND_WINDOW=50
VOLATILITY_THRESHOLD_HIGH=0.03
VOLATILITY_THRESHOLD_LOW=0.01
TREND_THRESHOLD=0.5

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=your-crypto-bot-bucket

# Web Dashboard
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PORT=5000
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=your_secure_password

# TensorFlow Configuration
TF_ENABLE_GPU=true
TF_GPU_MEMORY_LIMIT=4096
